<androidx.preference.PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.preference.PreferenceCategory
        app:title="@string/android_start"
        app:icon="@drawable/ic_category_start_tinted">
        <androidx.preference.Preference
            app:title="@string/android_start"
            app:summary="@string/summary_preference_android_start"
            app:icon="@drawable/ic_flag">
            <intent
                android:action="android.intent.action.VIEW"
                android:targetPackage="com.d4rk.androidtutorials"
                android:targetClass="com.d4rk.androidtutorials.ui.android.start.AndroidStartProjectActivity"/>
        </androidx.preference.Preference>
    </androidx.preference.PreferenceCategory>
    <androidx.preference.PreferenceCategory
        app:title="@string/basics"
        app:icon="@drawable/ic_category_build_tinted">
        <androidx.preference.Preference
            app:title="@string/shortcuts"
            app:summary="@string/summary_shortcuts"
            app:icon="@drawable/ic_app_shortcuts">
            <intent
                android:action="android.intent.action.VIEW"
                android:targetPackage="com.d4rk.androidtutorials"
                android:targetClass="com.d4rk.androidtutorials.ui.android.basics.shortcuts.ShortcutsActivity"/>
        </androidx.preference.Preference>
        <androidx.preference.Preference
            app:title="@string/android_sdk"
            app:summary="@string/summary_preference_android_studio_android_sdks"
            app:icon="@drawable/ic_android_sdk">
            <intent
                android:action="android.intent.action.VIEW"
                android:targetPackage="com.d4rk.androidtutorials"
                android:targetClass="com.d4rk.androidtutorials.ui.android.basics.sdk.AndroidSDK"/>
        </androidx.preference.Preference>
        <androidx.preference.Preference
            app:title="@string/view_binding"
            app:summary="@string/summary_preference_android_studio_view_binding"
            app:icon="@drawable/ic_code_binding">
