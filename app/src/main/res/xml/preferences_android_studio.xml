                android:targetPackage="com.d4rk.androidtutorials"
                android:targetClass="com.d4rk.androidtutorials.ui.android.basics.viewbinding.ViewBindingTutorialActivity"/>
        </androidx.preference.Preference>
        <androidx.preference.Preference
            app:title="@string/permissions"
            app:summary="@string/summary_preference_android_studio_permissions"
            app:icon="@drawable/ic_permissions_tutorial">
            <intent
                android:action="android.intent.action.VIEW"
                android:targetPackage="com.d4rk.androidtutorials"
                android:targetClass="com.d4rk.androidtutorials.ui.android.basics.permissions.PermissionsTutorialActivity"/>
