<me.zhanghai.android.fastscroll.FastScrollScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.gridlayout.widget.GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="24dp"
        app:columnCount="2">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_android_studio_tutorials_java"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:background="@android:color/transparent"
            android:clickable="true"
            android:focusable="true"
            android:src="@mipmap/ic_launcher_android_studio_tutorials_java"
            tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck"/>
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/text_view_android_studio_tutorials_java"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:gravity="center"
            android:textStyle="bold"
            android:text="@string/android_studio_tutorials_java"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_cart_calculator"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:background="@android:color/transparent"
            android:clickable="true"
            android:focusable="true"
            android:src="@mipmap/ic_launcher_cart_calculator"
            tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck"/>
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/text_view_cart_calculator"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:gravity="center"
            android:textStyle="bold"
            android:text="@string/cart_calculator"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_cleaner"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:src="@mipmap/ic_launcher_cleaner"
            android:background="@android:color/transparent"
            tools:ignore="SpeakableTextPresentCheck"/>
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/text_view_cleaner"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:gravity="center"
            android:textStyle="bold"
            android:text="@string/cleaner_plus"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_english_with_lidia"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:src="@mipmap/ic_launcher_english_with_lidia"
            android:background="@android:color/transparent"
            tools:ignore="SpeakableTextPresentCheck"/>
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/text_view_english_with_lidia"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:gravity="center"
            android:textStyle="bold"
            android:text="@string/english_with_lidia"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_low_brightness"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:src="@mipmap/ic_launcher_low_brightness"
            android:background="@android:color/transparent"
            tools:ignore="SpeakableTextPresentCheck"/>
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/text_view_low_brightness"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:gravity="center"
            android:textStyle="bold"
            android:text="@string/low_brightness"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_music_sleep_timer"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:background="@android:color/transparent"
            android:clickable="true"
            android:focusable="true"
            android:src="@mipmap/ic_launcher_music_sleep_timer"
            tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck"/>
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/text_view_music_sleep_timer"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:gravity="center"
            android:textStyle="bold"
            android:text="@string/music_sleep_timer"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_qr_and_code_scanner"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:src="@mipmap/ic_launcher_qr_codes_scanner"
            android:background="@android:color/transparent"
            tools:ignore="SpeakableTextPresentCheck"/>
        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/text_view_qr_code_scanner"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_margin="8dp"
            android:focusable="true"
            android:clickable="true"
            android:gravity="center"
            android:textStyle="bold"
            android:text="@string/qr_code_scanner"/>
    </androidx.gridlayout.widget.GridLayout>
</me.zhanghai.android.fastscroll.FastScrollScrollView>