<me.zhanghai.android.fastscroll.FastScrollScrollView
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:id="@+id/scroll_view">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TableLayout
            android:id="@+id/table_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:stretchColumns="*"
            android:showDividers="middle">
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    style="@style/TextAppearance.Material3.TitleLarge"
                    android:textAlignment="center"
                    android:text="@string/shortcuts_title"
                    android:layout_margin="16dp"
                    app:fontFamily="@font/font_poppins"/>
                <com.google.android.material.textview.MaterialTextView
                    style="@style/TextAppearance.Material3.TitleLarge"
                    android:textAlignment="center"
                    android:text="@string/description"
                    android:layout_margin="16dp"
                    app:fontFamily="@font/font_poppins"/>
            </TableRow>
            <TableRow
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="Alt + Insert"
                    tools:ignore="HardcodedText"/>
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="@string/generate_code"/>
            </TableRow>
            <TableRow
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="Ctrl + O"
                    tools:ignore="HardcodedText"/>
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="@string/override_methods"/>
            </TableRow>
            <TableRow
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="Ctrl + I"
                    tools:ignore="HardcodedText"/>
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="@string/implement_methods"/>
            </TableRow>
            <TableRow
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="Ctrl + Alt + T"
                    tools:ignore="HardcodedText"/>
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="@string/surround"/>
            </TableRow>
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="Ctrl + Y"
                    tools:ignore="HardcodedText"/>
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="@string/delete_line"/>
            </TableRow>
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="Ctrl +"
                    tools:ignore="HardcodedText"/>
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="@string/collapse_and_expand_code_block"/>
            </TableRow>
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="-/+"
                    tools:ignore="HardcodedText"/>
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="@string/collapse_and_expand_all_code_block"/>
            </TableRow>
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="Ctrl + Shift +"
                    tools:ignore="HardcodedText"/>
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="@string/duplicate_current_line"/>
            </TableRow>
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="-/+"
                    tools:ignore="HardcodedText"/>
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="@string/basic_code_completion"/>
            </TableRow>
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:divider="?attr/colorAccent"
                android:showDividers="middle">
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="Ctrl + D"
                    tools:ignore="HardcodedText"/>
                <com.google.android.material.textview.MaterialTextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_margin="16dp"
                    android:text="@string/smart_code_completion"/>
            </TableRow>
        </TableLayout>
        <com.google.android.gms.ads.AdView
            android:id="@+id/ad_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="24dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/table_layout"
            ads:adSize="BANNER"
            ads:adUnitId="ca-app-pub-5294151573817700/4974008668"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</me.zhanghai.android.fastscroll.FastScrollScrollView>